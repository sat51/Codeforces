import java.util.*;
public class sol{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int div =0;
        while(n-->0){
            int a = sc.nextInt();
            int b = sc.nextInt();
            int num = Math.abs(b-a);
            div = num/10;
            int rem = num%10;
            if(rem!=0){
                div+=1;
            }
            System.out.println(div);
        }
        
    }
}
